        Searchable ->
            search style

        Filterable ->
            filter style

        Displayed ->
            display style

        Sortable ->
            sort style

        Distinct ->
            distinct style

        DarkMode ->
            darkMode style

        LightMode ->
            lightMode style

        SystemThemeMode ->
            systemThemeMode style

        ChevronDown ->
            chevronDown style

        ChevronUp ->
            chevronUp style

        Folder ->
