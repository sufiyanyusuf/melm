module UI.Icons exposing (Icon(..), Style(..), buildIcon)

import Element exposing (Element)
import Html
import Svg exposing (..)
import Svg.Attributes exposing (clipRule, d, fill, fillRule, height, preserveAspectRatio, viewBox, width, xmlSpace)
import UI.Styles exposing (ColorHue, ColorIntensity, Config, color, hexColor)


type Style
    = Outline
    | Filled


type Icon
    = Close
    | Checkmark
    | Documents
    | ArrowUpDown
    | Pin
    | Switches
    | PieChart
    | SettingsGear
    | Retry
    | Trash
    | Dictionary
    | Block
    | Searchable
    | Filterable
    | Displayed
    | Sortable
    | Distinct
    | DarkMode
    | LightMode
    | SystemThemeMode
    | ChevronDown
    | ChevronUp
    | Folder


buildIcon : Icon -> Style -> Config -> ColorHue -> ColorIntensity -> Element msg
buildIcon icon style config hue intensity =
    let
        hexVal =
            hexColor hue intensity config

        attrs =
            buildAttrs hexVal

        path =
            getPath style icon
    in
    Element.html (svg attrs path)


buildAttrs : String -> List (Html.Attribute msg)
buildAttrs colorHexValue =
    [ width "24"
    , height "24"
    , viewBox "0 0 24 24"
    , fill colorHexValue
    , xmlSpace "http://www.w3.org/2000/svg"
    , preserveAspectRatio "xMaxYMid meet"
    ]


getPath : Style -> Icon -> List (Svg msg)
getPath style icon =
    case icon of
        Close ->
            close style

        Checkmark ->
            checkmark style

        Documents ->
            documents style

        ArrowUpDown ->
            arrowUpDown style

        Pin ->
            pin style

        Switches ->
            switches style

        PieChart ->
            pieChart style

        SettingsGear ->
